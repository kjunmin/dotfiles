// Place your key bindings in this file to override the defaults
[
  {
    "key": "cmd+i",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "editorFocus"
  },
  {
    "key": "cmd+w",
    "command": "workbench.action.terminal.kill",
    "when": "terminalFocus"
  },
  {
    "key": "cmd+i",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "ctrl+shift+i",
    "command": "workbench.action.splitEditor",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusPreviousGroup",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.focusNextGroup",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+j",
    "command": "terminal.focus",
    "when": "editorFocus && terminal.active"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "key": "alt+f",
    "command": "workbench.action.findInFiles"
  },
  {
    "key": "ctrl+f",
    "command": "search.action.focusNextSearchResult",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "key": "ctrl+enter",
    "command": "search.action.openInEditor",
    "when": "hasSearchResult && searchViewletFocus"
  },
  {
    "key": "cmd+q",
    "command": "editor.toggleFoldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "tab",
    "command": "workbench.action.nextEditor",
    "when": "editorTextFocus && vim.mode != 'Insert'"
  },
  {
    "key": "shift+tab",
    "command": "workbench.action.previousEditor",
    "when": "editorTextFocus && vim.mode != 'Insert'"
  },
  {
    "key": "tab",
    "command": "selectNextSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
  },
  {
    "key": "shift+tab",
    "command": "selectPrevSuggestion",
    "when": "suggestWidgetMultipleSuggestions && suggestWidgetVisible && textInputFocus || suggestWidgetVisible && textInputFocus && !suggestWidgetHasFocusedSuggestion"
  },
  {
    "key": "enter",
    "command": "acceptSelectedSuggestion",
    "when": "suggestWidgetHasFocusedSuggestion && suggestWidgetVisible && textInputFocus"
  },
  {
    "key": "cmd+alt+up",
    "command": "editor.action.moveLinesUpAction",
    "when": "editorTextFocus && !editorReadOnly"
  },
  {
    "key": "cmd+alt+down",
    "command": "editor.action.moveLinesDownAction",
    "when": "editorTextFocus && !editorReadOnly"
  },
  { "key": "cmd+i", "command": "workbench.action.moveEditorToNextGroup", "when": "editorFocus && terminalViewShowing" },
  { "key": "cmd+shift+i", "command": "workbench.action.moveEditorToPreviousGroup" },
  { "key": "shift+cmd+w", "command": "" },
  { "key": "cmd+j", "command": "" },
  {
    "key": "cmd+w",
    "command": "",
    "when": "!editorIsOpen && !multipleEditorGroups"
  }
]
